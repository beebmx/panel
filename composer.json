{
    "name": "beebmx/panel",
    "version": "2.0",
    "homepage": "https://github.com/beebmx/panel",
    "authors": [
        {
            "name": "Fernando Gutierrez",
            "email": "fernando@beeb.mx",
            "homepage": "http://beeb.mx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "^2.5",
        "symfony/yaml": "^3.1",
        "intervention/image": "^2.3"
    },
    "autoload": {
        "classmap" : [
            "src/lib",
            "src/controllers"
        ],
        "psr-4" : {
            "Beebmx\\Panel\\" : "src"
        }
    },
    "post-update-cmd": [
        "php artisan vendor:publish --provider=\"Beebmx\\Panel\\ServiceProvider\" --tag=config",
        "composer dump-autoload"
    ],
    "minimum-stability" : "stable"
}